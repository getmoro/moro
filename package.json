  "license": "MIT",
  "bugs": {
    "url": "https://github.com/omidfi/moro/issues"
  },
  "homepage": "https://github.com/omidfi/moro#readme",
  "dependencies": {
    "caporal": "^0.5.0",
    "chalk": "^1.1.3",
    "cli-table2": "^0.2.0",
    "expand-home-dir": "^0.0.3",
    "fs-extra": "^2.1.2",
    "jsonfile": "^2.4.0",
    "knex": "^0.12.7",
    "moment": "^2.17.1",
    "mz": "^2.6.0",
    "ora": "^1.2.0",
    "os-homedir": "^1.0.2",
    "sqlite3": "^3.1.8",
